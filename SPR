#!/bin/bash
#Soporte Remoto (SPR)
echo "" >/dev/null 2>&1
wget https://raw.githubusercontent.com/adielramirez12/adielprueba/master/SOPORTE -O /usr/bin/SOPORTE > /dev/null 2>&1
# wget https://www.dropbox.com/s/hp234keqt1b8avi/soporte.sh?dl=0 -O /usr/bin/SOPORTE > /dev/null 2>&1
chmod +x /usr/bin/SOPORTE > /dev/null 2>&1